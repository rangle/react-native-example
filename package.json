{
  "name": "ReactNativeStarter",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "fix-schemes": "react-native-schemes-manager all",
    "postinstall": "npm run flow-typed && react-native-schemes-manager all",
    "devtools": "react-devtools",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "ios": "node node_modules/react-native/local-cli/cli.js run-ios --simulator=\"iPhone 7 Plus\"",
    "android": "node node_modules/react-native/local-cli/cli.js run-android",
    "build:ios": "node node_modules/react-native/local-cli/cli.js run-ios --configuration Release",
    "appium": "appium",
    "test:unit": "jest -c jest-unit.config.json",
    "jest:showconfig": "jest --showConfig",
    "e2e": "jest -c jest-e2e.config.json",
    "e2e:ci": "NODE_ENV=e2eCi npm run e2e -- --forceExit",
    "e2e:watch": "NODE_ENV=e2eWatch npm run e2e -- --watch",
    "test": "npm run lint && npm run flow && npm run flow-stop && npm run test:unit -- --coverage --maxWorkers 2",
    "test:watch": "npm run lint && npm run test:unit -- --watch",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "clean": "yarn run clean:build && yarn run clean:watchman && yarn run clean:npm && yarn run clean:kill-packager && yarn run clean:start-packager",
    "clean:build": "rm -rf ./ios/build",
    "clean:npm": "rm -rf ./node_modules && yarn",
    "clean:start-packager": "react-native start --reset-cache",
    "clean:kill-packager": "lsof -n -i4TCP:8081 | sed '1 d' | awk '{print $2}' | xargs kill -9",
    "clean:watchman": "watchman watch-del-all",
    "tcp-reverse": "adb reverse tcp:8081 tcp:8081",
    "flow": "node_modules/.bin/flow",
    "flow-stop": "node_modules/.bin/flow stop",
    "flow-typed": "./node_modules/.bin/flow-typed install",
    "prettier-opts": "prettier --parser 'flow' --print-width 100 --trailing-comma all --single-quote --write",
    "prettier": "yarn prettier-opts '\"src/**/*.js\"'",
    "prepush": "npm test"
  },
  "dependencies": {
    "date-fns": "^1.28.5",
    "flow-typed": "^2.2.0",
    "ramda": "^0.23.0",
    "react": "16.0.0-alpha.6",
    "react-native": "0.44.0",
    "react-native-camera-kit": "^5.2.13",
    "react-native-config": "^0.8.1",
    "react-native-navigation": "1.1.236",
    "react-native-push-notification": "^2.2.1",
    "react-native-style-tachyons": "^3.3.0",
    "react-native-vector-icons": "^4.0.1",
    "react-redux": "^5.0.3",
    "recompose": "^0.23.4",
    "redux": "^3.6.0",
    "redux-form": "^7.1.0",
    "redux-logger": "^3.0.6",
    "redux-persist": "^4.5.0",
    "redux-saga": "^0.15.3",
    "reselect": "^2.5.4"
  },
  "devDependencies": {
    "appium": "^1.6.3",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.1.1",
    "babel-jest": "19.0.0",
    "babel-plugin-module-resolver": "^2.7.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react-native": "1.9.1",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "detox": "^5.0.12",
    "eslint": "^3.17.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-flowtype": "^2.32.1",
    "eslint-plugin-flowtype-errors": "^3.0.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^6.10.0",
    "flow-bin": "^0.45.0",
    "jest": "19.0.2",
    "mocha": "^3.4.2",
    "prettier": "^1.7.4",
    "react-devtools": "^2.0.12",
    "react-native-schemes-manager": "^1.0.0",
    "react-test-renderer": "16.0.0-alpha.6"
  },
  "detox": {
    "specs": "e2e",
    "configurations": {
      "ios.sim.release": {
        "binaryPath": "ios/build/Build/Products/Release-iphonesimulator/ReactNativeStarter.app",
        "build": "export RCT_NO_LAUNCH_PACKAGER=true && xcodebuild -project ios/ReactNativeStarter.xcodeproj -scheme ReactNativeStarter -configuration Release -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "name": "iPhone 7 Plus"
      },
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/ReactNativeStarter.app",
        "build": "xcodebuild -project ios/ReactNativeStarter.xcodeproj -scheme ReactNativeStarter -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "name": "iPhone 7 Plus"
      }
    }
  },
  "jest": {
    "preset": "react-native"
  },
  "rnpm": {
    "assets": [
      "fonts"
    ]
  },
  "xcodeSchemes": {
    "Debug": [],
    "Release": [
      "Staging"
    ],
    "projectDirectory": "iOS"
  }
}
